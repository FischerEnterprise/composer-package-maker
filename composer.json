{
    "name": "fischer-enterprise/composer-package-maker",
    "description": "CLI Tools to create composer packages",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Fischer",
            "email": "ben@fischer-enterprise.de"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "symfony/console": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/package-maker"
    ],
    "autoload": {
        "psr-4": {
            "FischerEnterprise\\ComposerPackageMaker\\": "src/",
            "FischerEnterprise\\ComposerPackageMaker\\Preset\\": "presets/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FischerEnterprise\\ComposerPackageMaker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
